279.95px){.jss31{display:flex}}@media (max-width:1279.95px){.jss32{display:none}}.jss33{height:auto;max-width:100px;margin-bottom:0!important}.jss34{height:auto;max-width:120px;margin-bottom:0!important}@media (min-width:960px){.jss35{display:none}}@media (max-width:1279.95px){.jss35{display:flex}}@media (min-width:960px){.jss36{display:none;margin-right:3%}}@media (min-width:1280px) and (max-width:1399.95px){.jss36{margin-right:0}}@media (max-width:1279.95px){.jss36{display:none!important}}.jss37{height:auto;max-width:100px;margin-bottom:0!important}.jss38{color:#000;font-size:1.14rem;text-decoration:none}.jss38:hover{color:orange;font-weight:700;text-decoration:none}.jss42{color:#fff;overflow:hidden;background-size:cover;background-image:url(https://static.chinhdaisteel.com/background/footer.png);background-repeat:no-repeat}@media (max-width:959.95px){.jss42{text-align:center}}.jss43{margin-top:3rem;padding-bottom:3rem}.jss44{color:#fff;font-size:1.14rem;line-height:1.5;text-decoration:none}.jss45{margin-bottom:10px!important}.jss46{display:flex;align-items:center;margin-bottom:10px!important;flex-direction:row}.jss47{display:flex;align-items:center;margin-bottom:0!important;flex-direction:row}@media (max-width:959.95px){.jss47{margin-left:auto;margin-right:auto}}.jss48{color:#fff;font-size:1.14rem;font-family:Roboto,Helvetica,Arial,san-serif;line-height:1.5;padding-left:1rem;text-decoration:none}.jss49{width:30px;height:30px}@media (max-width:1279.95px){.jss50{padding:2rem!important}}.jss39{font-size:1rem;font-family:Roboto,Helvetica,Arial,sans-serif;background-color:#f2ae1c}@media (max-width:959.95px){.jss39{width:100%}}@media (min-width:960px){.jss39{width:50%;height:3.5rem;margin-top:1rem;margin-left:25%}}.jss40{font-size:1.14rem;line-height:1.5}.jss41{color:#888;font-size:1.14rem;line-height:1.5}@media (max-width:1279.95px){.jss1{margin-top:40px;margin-bottom:80px}}@media (min-width:960px){.jss1{margin-top:80px;margin-bottom:80px}}.jss2{border-right:1px solid #ddd}@media (min-width:960px){.jss2{text-align:justify;padding-right:3rem!important}}.jss3{font-size:2rem;font-weight:700;line-height:1.5;text-transform:uppercase}@media (min-width:960px){.jss3{margin-top:8rem;margin-bottom:1rem}}@media (max-width:1279.95px){.jss3{margin-top:2rem}}.jss4{font-size:1.14rem;line-height:1.5}.jss5{color:#337ab7;font-size:1.14rem;font-family:Roboto,Helvetica,Arial,san-serif;text-decoration:none}.jss6{color:#337ab7;font-size:1.14rem;font-family:Roboto,Helvetica,Arial,san-serif;text-decoration:none}.jss7{color:#b4b5bb;font-size:1.14rem;font-family:Roboto,Helvetica,Arial,san-serif}.jss8{color:#b4b5bb;font-size:1.14rem;font-family:Roboto,Helvetica,Arial,san-serif}.jss9{margin-bottom:48px}@media (max-width:1279.95px){.jss9{display:none}}.jss10{align-items:left}.jss11{width:50%}.jss12{display:flex;align-items:center;font-family:Roboto,Helvetica,Arial,san-serif;margin-left:auto;margin-right:auto;margin-bottom:96px}.jss13{font-size:1.71rem;margin-top:32px;margin-bottom:24px}.jss14{font-size:1.14rem;margin-top:64px;font-weight:700}@media (max-width:1279.95px){.jss14{display:none}}.jss15{display:flex;align-items:center;margin-bottom:24px;justify-content:center}@media (max-width:1279.95px){.jss15{display:none}}.jss16{color:#fff;height:100%;font-size:1.14rem;font-family:Roboto,Helvetica,Arial,san-serif;padding-top:12px;padding-left:20px;border-radius:3px;padding-right:20px;padding-bottom:12px;background-color:#f2ae1c}.jss17{height:auto;max-width:80%;margin-left:auto;margin-right:auto}.jss18{color:#615b5b;font-size:1rem;border-bottom:1px solid #ddd;padding-bottom:1rem;text-transform:uppercase}.jss19{margin-bottom:3rem}</style><title data-rh="true"></title><meta data-rh="true" name="robots" content="index,follow"/><meta data-rh="true" name="googlebot" content="index,follow"/><meta data-rh="true" name="twitter:card" content="summary_large_image"/><meta data-rh="true" name="twitter:site" content="chinhdaisteelvn"/><meta data-rh="true" name="twitter:crea